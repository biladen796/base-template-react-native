{
  "name": "templateapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios",
    "ios-staging-mac": "ENVFILE=.staging.env npx react-native run-ios",
    "android-staging-mac": "ENVFILE=.staging.env npx react-native run-android",
    "ios-staging-window": "SET ENVFILE=.staging.env && npx react-native run-ios",
    "android-staging-window": "SET ENVFILE=.staging.env && npx react-native run-android",
    "yarn-ios": "yarn && yarn pod",
    "android-rl": "npx react-native run-android --variant=release",
    "start": "npx react-native start --reset-cache",
    "link": "npx react-native link",
    "clean": "cd android && ./gradlew clean && cd ..",
    "apk": "cd android && ./gradlew clean && ./gradlew assembleRelease && cd ..",
    "aab": "cd android && ./gradlew clean && ./gradlew bundleRelease && cd ..",
    "pod": "cd ios && pod install && cd ..",
    "test": "jest",
    "eslint": "eslint --max-warnings=0 . && prettier --check .",
    "eslint-fix": "eslint --fix . && prettier --write .",
    "check-types": "npx tsc --noemit",
    "lint": "yarn eslint-fix && yarn eslint && yarn check-types",
    "update-ios": "appcenter codepush release-react -a ACS-SHINHAN/Paperless-iOS -d Staging -m -t",
    "update-android": "appcenter codepush release-react -a ACS-SHINHAN/Paperless-Android -d Staging -m -t"
  },
